{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
    },
    "content": {
      "type": "richtext"
    },
    "authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::author.author"
    },
    "description": {
      "type": "string",
      "required": true
    },
    "site": {
      "type": "enumeration",
      "enum": [
        "XPRTZ",
        "XPRTZ TOO",
        "XPRTZ Cloud"
      ],
      "required": true,
      "default": "XPRTZ"
    }
  }
}
